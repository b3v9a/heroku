extends layout

block content
	.page-header
		h1 Your Comic Drafts
	.row
		each comic, i in comics
			if comic.publish == false
				.col-md-4.col-sm-6.col-xs-12
					.panel.panel-default
						.panel-heading <a href="#{comic.comicSource}">#{comic.comicTitle}</a>
						.panel-body <a href="/view/#{comic._id}"><img height="25%" src="#{comic.panels[0].source}"></img></a>